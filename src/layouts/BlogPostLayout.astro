---
import Layout from "./Layout.astro"

interface Props {
  title: string
}

const { title } = Astro.props
---

<Layout title={title}>
  <div class="m-6 md:m-10">
    <div
      class="prose mx-auto w-full !max-w-4xl dark:prose-invert prose-p:text-justify prose-a:text-blue-600"
    >
      <slot />
    </div>
  </div>
</Layout>
